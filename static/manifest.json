{
    "manifest_version": 3,
    "name": "NoCookiesOnlyMilk",
    "description": "Remove annoying cookie popups from websites.",
    "version": "1.0.0",
    "action": {
        "default_icon": {
            "16": "images/logo-16.png",
            "24": "images/logo-24.png",
            "32": "images/logo-32.png"
        }
    },
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}
